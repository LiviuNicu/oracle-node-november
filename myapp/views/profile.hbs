<h1>{{title}}</h1>
<h4>Change profile image for {{selectedUser.firstName}} {{selectedUser.lastName}}</h4>

<form action="/users/upload" method="post" enctype="multipart/form-data">
    <input type="hidden" name="userID" value="{{selectedUser._id}}" />
    <input type="file" name="sampleFile" />
    <input type="submit" value="'Upload!'" />
</form>
